int main(){
    int a, result=0, i;
    printf("Enter a number: ");
    scanf("%d",&a);
    if (a==1 || a==0){
        printf("Perfect square");
    }
    for (i=2;i<a/2;i++){
        if (a==i*i){
            printf("Perfect square");
            result=1;
            break;
        }
    }
    if (result==0){
        printf("Not");
    }
    return 0;
}
